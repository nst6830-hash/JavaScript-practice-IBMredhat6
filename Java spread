<!DOCTYPE html>
<html lang="en">
<head>
</head>
<body>

<script>
    //... : 스프레드 구문 **(react)** : 배열의 내부값을 순서대로 펼친다.

//     const ary=[1,2,3,4,5,6];
//     console.log(ary); //(2) [1,2,3,4,5,6]
//     console.log(...ary);// 1 2 3 4 5 6 
    

//     const fucn1=(a,b)=>{
//     console.log(a+b);
// }
    //fucn1(ary[0],ary[1])
    //fucn1(...ary) //ary 배열을 펼쳐 1,2,가 index순서에 의거해 각 a,b 매개변수로 삽입 

    const ary2=[1,2,3,4,5];
    const [a,...c]=ary2; //배열에서 남은 요소를 변수에 담을 때 스프레드 구문 활용
     //^위 코드의 뜻은 인덱스 첫번째는 a에 담고, 나머지는 c에 담는다
    console.log(a); //1
    console.log(c);//(4){2,3,4,5}

    const[a1,b1,c1,d1,e1]=ary2;//[1,2,3,4,5];
    console.log(e1); //5
    //ary2가 1~5까지의 배열이 있으니 e1다음으로 f1을  적어도 undefined로 출력된다.

    const ary3=[10,20];
    const ary4=[30,40];
              //...을 넣어서 
    const ary5=[...ary3]; //얕은 복사 : 객체의 주소값(참조값)을 복사 (껍데기 복사)
    //얕은복사 : 객체의 주소값(참조값)만 복사 //깊은복사 : 객체의 실질적 데이터 복사(value)
    console.log(ary3);//[10,20]
    console.log(ary5);//[10,20]
    
    ary5[0]=100;
    console.log(ary3); //[10,20]
    console.log(ary5); //[100,20] 겉만 복사 한 상태

    const ary6=[...ary3,...ary4];
    console.log(ary6); //[10,20,30,40]
///////////////////////////////////////////////////////////////////////
//...스프레드(spread)구문 : iterable 반복 가능한.(배열,객체,문자열)
  const obj={
        v1:10,
        v2:20
    }
    

    const obj2={
        v1:30,
        v2:40
    }

    const obj3={...obj};
    console.log(obj3); //{v1 : 10, v2 : 20}

    //obj1과 obj2 다 펼쳐서 obj4에 저장 후 출력

    const obj4={...obj, ...obj2};
    console.log(obj4);

    const ary8=[...ary3];
    ary8[0]=100; //스프레드 구문 사용하면 -> 완전한 새로운 배열 생성 -> 기존배열에 영향없음
    console.log(ary8);
    console.log(ary3); //겉만 복사
    
    

    
    
    

</script>

</body>


</html>
