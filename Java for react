        let n='let 변수';
        console.log(n);

        n='let 변수 다시 수정';
        console.log(n);

        //let 덮어쓰기가 가능하나 재선언 불가능

        const n2='const변수';
        console.log(n2);
        //const 덮어쓰기(변수 재할당), 재선언 둘 다 불가능 
        //const 기본타입 = 덮어쓰기 불가능



        //      객체타입 = 덮어쓰기 가능
              const obj={
            name:'lee',
            age:22
        }
        obj.name='kim'
        console.log(obj.name);
        
      
        const ary1=['a','b','c']
        ary1[0] = 'd'
        // 배열 또한 객체이므로 덮어쓰기 가능
      
        ary1.push('e')
              
        
        <!DOCTYPE html>
<html lang="en">
<head>
</head>
<body>
<!-- js 리액트에서 많이 쓰는 문법 -->
    <script>


        const name1='tom'
        const age = 30;

        const msg = `내이름은${name1}이고 나이는 ${age}살이다`;
        console.log(msg);
        
        function hi(){
            return "tom";

        }

        const msg2=`인사해줘 ${hi()}~${age+2}~`
        console.log(msg2);
        //백틱 ` 에서도 함수 호출 가능 기억해두자




    </script>

  
</body>

</html>

