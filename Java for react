        let f=document.querySelector(".frame")
        let n=f.childNodes.length;

        f.childNodes[1].style.color='red'//배열
        //text 노드 (줄바꿈 + 공백)
        //<h1>
        //text 
        //<p>
        //text
        //<button>
        //text
            let m=f.children.length;
            alert(m)
            //요소 노드 칠드런과 차일드노드
    </script>

  
